<jsp:include page="../header.jsp">
    <jsp:param value="Accomodation Details" name="pageTitle"/>
</jsp:include>
<%@taglib uri="http://www.springframework.org/tags/form" prefix="spring"%>
<%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<spring:form modelAttribute="commonBean" action="next.htm">
<br></br>
	<table width="65%" class="menu_box" align="center">
    <tr bgcolor="#c0c0c0"><td>&nbsp;</td><td>&nbsp;</td>
    <td align="center"><b><font color="#000000">Arrivals Registration</font></b></td>
    <td>&nbsp;</td><td>&nbsp;</td>
    </tr>
	<tr>
	<td align="left"><spring:label path="arrName" >Arrivals Name:<font color="red">*</font></spring:label></td>
	<td align="left"><spring:input path="arrName" /></td>
	<td><spring:errors path="arrName" cssClass="errorMessage"/></td>
	<td align="left"><spring:label path="arrSpritualName">Spritual Name:</spring:label></td>
	<td align="left"><spring:input path="arrSpritualName" /></td>
	<td>&nbsp;</td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="headName">Group Head Name:</spring:label></td>
	<td align="left"><spring:input path="headName" /></td>
	<td>&nbsp;</td>
	<td align="left"><spring:label path="arrSex">Sex:</spring:label></td>
	<td align="left">
	<spring:select path="arrSex">
		<option value="Male" selected="selected">Male</option>
		<option value="Female">Female</option>
	</spring:select>
	</td>
	<td>&nbsp;</td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="dateOfArrivals" >Date Of Arrivals:<font color="red">*</font></spring:label></td>
	<td align="left"><spring:input path="dateOfArrivals" readonly="true"/></td>
	<td><spring:errors path="dateOfArrivals" cssClass="errorMessage"/></td>
	<td align="left"><spring:label path="dateOfDeparture">Date Of Departure:</spring:label></td>
	<td align="left"><spring:input path="dateOfDeparture" /></td>
	<td>&nbsp;</td>
	</tr>
	<tr>
	<td align="left"><spring:label path="dateOfBirth" >Date Of Birth:</spring:label></td>
	<td align="left"><spring:input path="dateOfBirth" readonly="true"/></td>
	<td>&nbsp;</td>
	<td align="left"><spring:label path="emailId">Email Id:</spring:label></td>
	<td align="left"><spring:input path="emailId" /></td>
	<td>&nbsp;</td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="coutry">Country:<font color="red">*</font></spring:label></td>
	<td align="left"><spring:input path="coutry" /></td>
	<td><spring:errors path="coutry" cssClass="errorMessage"/></td>
	<td align="left"><spring:label path="mobileNumber" >Contact No.:</spring:label></td>
	<td align="left"><spring:input path="mobileNumber" /></td>
	<td></td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="state" >State:<font color="red">*</font></spring:label></td>
	<td align="left"><spring:input path="state" /></td>
	<td><spring:errors path="state" cssClass="errorMessage"/></td>
	<td align="left"><spring:label path="addressLine1" >Address</spring:label></td>
	<td align="left"><spring:textarea  path="addressLine1" /></td>
	<td>&nbsp;</td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="place">Place:<font color="red">*</font></spring:label></td>
	<td align="left"><spring:input path="place" /></td>
	<td><spring:errors path="place" cssClass="errorMessage"/></td>
	<td align="left"><spring:label path="zipCode" >Zip Code:</spring:label></td>
	<td align="left"><spring:input path="zipCode" /></td>
	<td>&nbsp;</td>
	</tr>
	
	<tr>
	<td align="left"><spring:label path="arrKarmaYoga">KarmaYoga:</spring:label></td>
	<td align="left"><spring:input path="arrKarmaYoga" /></td>
	<td>&nbsp;</td>
	<td align="left"><spring:label path="purposeOfVisit">Purpose Of Visit:</spring:label></td>
	<td align="left"><spring:input path="purposeOfVisit" /></td>
	<td>&nbsp;</td>
	</tr>	
	<tr>
	<table align="center">
	<tr>
	<td align="left"><input type="button" value="Back"
				onclick="window.history.back()"/></td>
	<td align="right"><input type="submit" value="Next" /></td>
	</tr></table>
	</tr>
	
   </table>
</spring:form>
<%@include file="../footer.jsp" %>